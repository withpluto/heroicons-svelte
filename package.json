      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.2",
    "@sveltejs/package": "^2.0.0",
    "chalk": "^5.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0"
  },
  "svelte": "./dist/index.js",
